<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #DB8270;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.graphics.pages.SettingPageFXML">
   <children>
      <ImageView fitHeight="387.0" fitWidth="700.0" layoutY="58.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Images/bg2.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="67.0" layoutY="14.0" prefHeight="440.0" prefWidth="618.0">
         <children>
            <Label layoutX="66.0" layoutY="70.0" text="Activity" textFill="#666666">
               <font>
                  <Font name="Cambria Bold Italic" size="20.0" />
               </font>
            </Label>
            <Label layoutX="66.0" layoutY="155.0" text="Privacy" textFill="#666666">
               <font>
                  <Font name="Cambria Bold Italic" size="20.0" />
               </font>
            </Label>
            <Label layoutX="66.0" layoutY="260.0" text="Last Seen &amp; Online" textFill="#666666">
               <font>
                  <Font name="Cambria Bold Italic" size="20.0" />
               </font>
            </Label>
            <RadioButton fx:id="ActiveButton" layoutX="57.0" layoutY="105.0" mnemonicParsing="false" onAction="#active" text="Active">
               <font>
                  <Font name="Cambria Italic" size="16.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="DiactiveButton" layoutX="221.0" layoutY="105.0" mnemonicParsing="false" onAction="#Diactive" text="Diactive">
               <font>
                  <Font name="Cambria Italic" size="16.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="PublicButton" layoutX="57.0" layoutY="199.0" mnemonicParsing="false" onAction="#Public" text="Public">
               <font>
                  <Font name="Cambria Italic" size="16.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="PrivateButton" layoutX="224.0" layoutY="199.0" mnemonicParsing="false" onAction="#Private" text="Private">
               <font>
                  <Font name="Cambria Italic" size="16.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="LastSeenEveryButton" layoutX="50.0" layoutY="307.0" mnemonicParsing="false" onAction="#lastSeenEverybody" text="Everybody">
               <font>
                  <Font name="Cambria Italic" size="16.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="LastSeenFollowerButton" layoutX="214.0" layoutY="307.0" mnemonicParsing="false" onAction="#lastSeenFollower" text="My followers">
               <font>
                  <Font name="Cambria Italic" size="16.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="LastSeenNobody" layoutX="379.0" layoutY="307.0" mnemonicParsing="false" onAction="#lastSeenNobody" text="Nobody">
               <font>
                  <Font name="Cambria Italic" size="16.0" />
               </font>
            </RadioButton>
            <Label fx:id="lblUsername" layoutX="58.0" layoutY="5.0" prefHeight="33.0" prefWidth="166.0" textFill="#666666">
               <font>
                  <Font name="Cambria Bold Italic" size="20.0" />
               </font>
            </Label>
            <Button fx:id="DoneButton" layoutX="221.0" layoutY="349.0" mnemonicParsing="false" onAction="#done" prefHeight="45.0" prefWidth="99.0" text="Done" textFill="#ff6600">
               <font>
                  <Font name="Cambria Bold Italic" size="18.0" />
               </font>
            </Button>
            <Label fx:id="WarningLabel" layoutX="131.0" layoutY="35.0" prefHeight="34.0" prefWidth="320.0" textFill="RED">
               <font>
                  <Font name="Cambria Bold" size="19.0" />
               </font>
            </Label>
            <Label fx:id="LastSeenLabel" layoutX="328.0" layoutY="258.0" prefHeight="22.0" prefWidth="52.0" visible="false">
               <font>
                  <Font name="Cambria Italic" size="12.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="BackButton" layoutX="597.0" layoutY="50.0" mnemonicParsing="false" onAction="#back" prefHeight="34.0" prefWidth="70.0" text="Go back" textFill="#ff6600">
         <font>
            <Font name="Cambria Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="MainMenuButton" layoutX="568.0" layoutY="425.0" mnemonicParsing="false" onAction="#mainMenu" prefHeight="45.0" prefWidth="99.0" text="Main Menu" textFill="#ff6600">
         <font>
            <Font name="Cambria Bold Italic" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
