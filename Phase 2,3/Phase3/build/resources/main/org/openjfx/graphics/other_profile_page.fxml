<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.graphics.pages.OtherProfilePageFXML">
   <children>
      <ImageView fitHeight="500.0" fitWidth="700.0" layoutY="32.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Images/bg3.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="58.0" layoutY="52.0" prefHeight="367.0" prefWidth="566.0">
         <children>
            <Label fx:id="UsernameLabel" layoutX="72.0" layoutY="44.0" prefHeight="38.0" prefWidth="208.0" textFill="#ff6600">
               <font>
                  <Font name="Cambria Bold Italic" size="30.0" />
               </font>
            </Label>
            <Label layoutX="106.0" layoutY="102.0" text="last seen:">
               <font>
                  <Font name="Cambria Italic" size="15.0" />
               </font>
            </Label>
            <Label fx:id="LastSeenLabel" layoutX="201.0" layoutY="102.0" prefHeight="20.0" prefWidth="158.0">
               <font>
                  <Font name="Cambria Italic" size="15.0" />
               </font>
            </Label>
            <Button fx:id="FollowButton" layoutX="414.0" layoutY="53.0" mnemonicParsing="false" onAction="#follow" prefHeight="38.0" prefWidth="98.0" text="Follow" textFill="#ff6600">
               <font>
                  <Font name="Cambria Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="BlockButton" layoutX="464.0" layoutY="287.0" mnemonicParsing="false" onAction="#block" prefHeight="30.0" prefWidth="58.0" text="Block">
