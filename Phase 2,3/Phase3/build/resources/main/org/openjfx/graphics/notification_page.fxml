<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #DB8270;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.graphics.pages.NotificationPageFXML">
   <children>
      <Label layoutX="85.0" layoutY="58.0" text="NOTIFICATIONS" textFill="#666666">
         <font>
            <Font name="Cambria Bold Italic" size="30.0" />
         </font>
      </Label>
      <Label layoutX="140.0" layoutY="126.0" text="Requests">
         <font>
            <Font name="Cambria Italic" size="22.0" />
         </font>
      </Label>
      <TextArea fx:id="RequestsList" editable="false" layoutX="92.0" layoutY="164.0" prefHeight="217.0" prefWidth="214.0" promptText="no requests yet" wrapText="true" />
      <TextField fx:id="UsernameText" layoutX="366.0" layoutY="218.0" promptText="Type their username" />
      <Button fx:id="AcceptButton" layoutX="533.0" layoutY="218.0" mnemonicParsing="false" onAction="#accept" text="Accept" />
      <Button fx:id="DenyButton" layoutX="617.0" layoutY="218.0" mnemonicParsing="false" onAction="#deny" text="Deny" />
      <Button fx:id="GoBackButton" layoutX="591.0" layoutY="45.0" mnemonicParsing="false" onAction="#back" text="Go Back" textFill="#ff6600">
         <font>
            <Font name="Cambria Italic" size="15.0" />
         </font>
      </Button>
      <Button fx:id="MenuButton" layoutX="585.0" layoutY="432.0" mnemonicParsing="false" onAction="#menu" text="Main Menu" textFill="#ff6600">
         <font>
            <Font name="Cambria Italic" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
