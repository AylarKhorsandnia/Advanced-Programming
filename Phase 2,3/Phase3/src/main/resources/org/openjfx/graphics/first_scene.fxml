<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: 0;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.graphics.pages.FirstSceneFXML">
   <children>
      <ImageView fitHeight="534.0" fitWidth="733.0" layoutX="87.0" layoutY="-27.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../Images/hola.jpg" />
         </image>
      </ImageView>
      <Button fx:id="SignUpButton" layoutX="169.0" layoutY="421.0" mnemonicParsing="false" onAction="#signUp" prefHeight="40.0" prefWidth="89.0" text="Sign up" textFill="#ff6600">
         <font>
            <Font name="Cambria Italic" size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="LoginButton" layoutX="443.0" layoutY="421.0" mnemonicParsing="false" onAction="#login" prefHeight="40.0" prefWidth="89.0" text="Login" textFill="#ff6600">
         <font>
            <Font name="Cambria Italic" size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
